{
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^22.14.13",
    "tailwindcss": "^3.3.6"
  },
  "name": "electron-login",
  "productName": "Electron Login",
  "description": "This is a sample login using elecronJS.",
  "version": "1.0.0",
  "main": "tailwind.config.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.Dhananga_008.electronLogin",
    "productName": "electronLogin",
    "directories": {
      "output": "dist"

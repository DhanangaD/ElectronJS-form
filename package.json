  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DhanangaD/Electron-Login/issues"
  },
  "homepage": "https://github.com/DhanangaD/Electron-Login#readme",
  "dependencies": {
    "better-sqlite3-with-prebuilds": "^7.4.3",
    "electron-packager": "^17.1.2",
    "sqlite3": "^5.1.6"
  },
  "win": {
    "target": "nsis",
    "icon": "./src/icon.ico"
  }
